							->plate;;
						*);
						<=nrel_stage_products:...
						(*
							->cutlets_e1;;
							->cutlets_e2;;
						*);
						=>nrel_processing_method: stirring;
						=>nrel_final_product: cutlets_e3;
						=>nrel_sequencing: concept_cutlets_recept_stage4;;

concept_cutlets_recept_stage4 <- concept_cutlets_recept_stages;
						<- stage;
						<=nrel_equipment:...
						(*
							->plate;;
						*);
						<=nrel_stage_products:...
