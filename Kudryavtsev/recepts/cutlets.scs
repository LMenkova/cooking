(*
	<=nrel_value:...
	(*
		->rrel_minutes:40;;
	*);;
*);;
concept_cutlets_recept ->rrel_1: concept_cutlets;;
concept_cutlets_recept <=nrel_stage_decomposition: concept_cutlets_recept_stages;;

concept_cutlets_recept_stage1 <-rrel_1: concept_cutlets_recept_stages;
						<- stage;
						<=nrel_equipment:...
						(*
							->chopper;;
							->plate;;
						*);
