concept_cutlets_recept <=nrel_context_of_action: [*
	concept_cutlets <-dish;;
	concept_cutlets=>nrel_ingredients:
		concept_chicken_fillet;
		concept_butter;
		concept_pepper;
		concept_egg;
		concept_salt;
                concept_flour;
                concept_vegetable_oil;;
*];;

